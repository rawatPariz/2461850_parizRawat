      "cell_type": "code",
      "source": [
        "df_cls = df_cls_raw[df_cls_raw[\"loan_status\"].isin([\"Fully Paid\", \"Charged Off\"])].copy()\n",
        "\n",
        "df_cls[\"target\"] = df_cls[\"loan_status\"].map({\n",
        "    \"Fully Paid\": 0,\n",
