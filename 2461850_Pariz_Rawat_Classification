    {
      "cell_type": "code",
      "source": [
        "from sklearn.model_selection import GridSearchCV\n",
        "\n",
        "param_grid_log = {\"model__C\": [0.01, 0.1, 1, 10]}\n",
        "grid_log = GridSearchCV(log_model, param_grid_log, cv=3, scoring=\"f1\", n_jobs=-1)\n",
        "grid_log.fit(X_train, y_train)\n",
        "\n",
        "best_log = grid_log.best_estimator_\n",
        "pred_log_tuned = best_log.predict(X_test)\n",
        "\n",
        "print(\"Best Logistic Params:\", grid_log.best_params_)\n",
