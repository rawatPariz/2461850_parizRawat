      "source": [
        "from sklearn.metrics import confusion_matrix\n",
        "\n",
        "def plot_cm(y_true, y_pred, title):\n",
        "    cm = confusion_matrix(y_true, y_pred)\n",
        "    plt.figure(figsize=(5,4))\n",
        "    sns.heatmap(cm, annot=True, fmt=\"d\", cmap=\"Blues\")\n",
        "    plt.title(title)\n",
        "    plt.xlabel(\"Predicted\")\n",
        "    plt.ylabel(\"Actual\")\n",
        "    plt.show()\n",
        "\n",
        "plot_cm(y_test, pred_log, \"Confusion Matrix - Logistic Regression\")\n",
        "plot_cm(y_test, pred_dt, \"Confusion Matrix - Decision Tree\")\n",
        "plot_cm(y_test, pred_mlp, \"Confusion Matrix - MLP Neural Network\")\n"
