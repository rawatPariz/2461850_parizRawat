        "**cleaning missing values**"
      ],
      "metadata": {
        "id": "0dFeYgDMPD9H"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# Drop heavy text columns\n",
        "df_cls = df_cls.drop([\"emp_title\", \"title\"], axis=1)\n",
        "\n",
        "# Fill numeric missing values\n",
        "df_cls[\"mort_acc\"] = df_cls[\"mort_acc\"].fillna(df_cls[\"mort_acc\"].median())\n",
        "df_cls[\"revol_util\"] = df_cls[\"revol_util\"].fillna(df_cls[\"revol_util\"].median())\n",
        "df_cls[\"pub_rec_bankruptcies\"] = df_cls[\"pub_rec_bankruptcies\"].fillna(df_cls[\"pub_rec_bankruptcies\"].median())\n",
        "\n",
        "# Fill categorical missing values\n",
        "df_cls[\"emp_length\"] = df_cls[\"emp_length\"].fillna(df_cls[\"emp_length\"].mode()[0])\n",
        "\n",
        "# Check missing values again\n",
        "df_cls.isna().sum().sort_values(ascending=False).head(10)\n"
